te path=\"/registration\" render={() => (\n          <MainLayout>\n            <Registration />\n          </MainLayout>\n        )} />\n        <Route path=\"/login\"\n          render={() => (\n            <MainLayout>\n              <Login />\n            </MainLayout>\n          )} />\n        <Route path=\"/recovery\" render={() => (\n          <MainLayout>\n            <Recovery />\n          </MainLayout>\n        )} />\n        <Route path=\"/dashboard\" render={() => (\n          <WithAuth>\n            <MainLayout>\n              <Dashboard />\n            </MainLayout>\n          </WithAuth>\n        )} />\n      </Switch>\n    </div>\n  );\n}\n\nconst mapStateToProps = ({ user }) => ({\n  currentUser: user.currentUser\n});\n\nconst mapDispatchToProps = dispatch => ({\n  setCurrentUser: user => dispatch(setCurrentUser(user))\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(App);\n","/Users/matthewgilligan/Code/matthewgilligan/gina-corrieri/src/components/header/index.js",[],["123","124"],"/Users/matthewgilligan/Code/matthewgilligan/gina-corrieri/src/pages/homepage/index.js",[],"/Users/matthewgilligan/Code/matthewgilligan/gina-corrieri/src/components/Header/index.js",[],"/Users/matthewgilligan/Code/matthewgilligan/gina-corrieri/src/pages/Homepage/index.js",[],"/Users/matthewgilligan/Code/matthewgilligan/gina-corrieri/src/pages/Registration/index.js",[],"/Users/matthewgilligan/Code/matthewgilligan/gina-corrieri/src/layouts/MainLayout.js",[],"/Users/matthewgilligan/Code/matthewgilligan/gina-corrieri/src/components/Footer/index.js",[],"/Users/matthewgilligan/Code/matthewgilligan/gina-corrieri/src/pages/Login/index.js",[],"/Users/matthewgilligan/Code/matthewgilligan/gina-corrieri/src/components/SignIn/index.js",[],"/Users/matthewgilligan/Code/matthewgilligan/gina-corrieri/src/components/forms/Button/index.js",[],"/Users/matthewgilligan/Code/matthewgilligan/gina-corrieri/src/firebase/utils.js",[],"/Users/matthewgilligan/Code/matthewgilligan/g