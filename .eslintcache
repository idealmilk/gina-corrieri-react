Name } = currentUser;\n\n  return (\n    <div className=\"userProfile\">\n      <ul>\n        <li>\n          <div className=\"img\">\n            <img src={userIMG} />\n          </div>\n        </li>\n        <li>\n          <span className=\"displayName\">\n            {displayName && displayName}\n          </span>\n        </li>\n      </ul>\n    </div>\n  );\n}\n\nexport default UserProfile;\n","/Users/matthewgilligan/Code/freelance/gina-corrieri/src/layouts/DashboardLayout.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/components/Modal/index.js",["226"],"import React, { useState } from 'react';\nimport './styles.scss';\n\nconst Modal = ({ hideModal, toggleModal, children }) => {\n  if (hideModal) return null;\n\n  return (\n    <>\n      <div className=\"modalOverlay\" onClick={() => toggleModal()} />\n      <div className=\"modal\">\n        {children}\n      </div>\n    </>\n  );\n}\n\nexport default Modal;\n","/Users/matthewgilligan/Code/freelance/gina-corrieri/src/components/forms/FormSelect/index.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/redux/Products/products.actions.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/redux/Products/products.types.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/redux/Products/products.sagas.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/redux/Products/products.helpers.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/redux/Products/products.reducer.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/pages/Search/index.js",["227"],"import React from 'react';\nimport ProductResults from './../../components/ProductResults'\n\nconst Search = ({ }) => {\n  return (\n    <div className=\"search-page\">\n      <ProductResults />\n    </div>\n  );\n};\n\nexport default Search;\n","/Users/matthewgilligan/Code/freelance/gina-corrieri/src/components/ProductResults/index.js",["228","229"],"import React, { useEffect } from 