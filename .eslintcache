unk from 'redux-thunk';\nimport createSagaMiddle from 'redux-saga';\nimport { persistStore } from 'redux-persist';\n\nimport rootReducer from './rootReducer';\nimport rootSaga from './rootSaga';\n\nconst sagaMiddleware = createSagaMiddle();\nexport const middlewares = [thunk, sagaMiddleware, logger];\n\nexport const store = createStore(rootReducer, applyMiddleware(...middlewares));\nsagaMiddleware.run(rootSaga);\n\nexport const persistor = persistStore(store);\n\nexport default {\n  store,\n  persistor\n};\n","/Users/matthewgilligan/Code/freelance/gina-corrieri/src/firebase/utils.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/redux/User/user.actions.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/layouts/MainLayout.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/hoc/withAuth.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/redux/rootSaga.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/redux/rootReducer.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/pages/Homepage/index.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/pages/Dashboard/index.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/pages/Registration/index.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/pages/Recovery/index.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/pages/Login/index.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/firebase/config.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/redux/User/user.types.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/redux/User/user.reducer.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/customHooks/index.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/components/Header/index.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/components/Footer/index.js",[],"/Users/matthewgilligan/Code/freelance/gina-corrieri/src/c