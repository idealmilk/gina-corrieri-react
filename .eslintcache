this.state;\n\n    const configAuthWrapper = {\n      headline: 'Sign Up'\n    };\n\n    return (\n      <AuthWrapper {...configAuthWrapper}>\n        <div className=\"form-wrap\">\n          {errors.length > 0 && (\n            <ul>\n              {errors.map((err, index) => {\n                return (\n                  <li key={index}>{err}</li>\n                )\n              })}\n            </ul>\n          )}\n          <form onSubmit={this.handleFormSubmit}>\n            <FormInput\n              type=\"text\"\n              name=\"displayName\"\n              value={displayName}\n              placeholder=\"Full Name\"\n              onChange={this.handleChange}\n            />\n            <FormInput\n              type=\"email\"\n              name=\"email\"\n              value={email}\n              placeholder=\"Email Address\"\n              onChange={this.handleChange}\n            />\n            <FormInput\n              type=\"password\"\n              name=\"password\"\n              value={password}\n              placeholder=\"Password\"\n              onChange={this.handleChange}\n            />\n            <FormInput\n              type=\"password\"\n              name=\"confirmPassword\"\n              value={confirmPassword}\n              placeholder=\"Confirm Password\"\n              onChange={this.handleChange}\n            />\n            <Button type=\"submit\">\n              Sign Up\n            </Button>\n          </form>\n        </div>\n      </AuthWrapper>\n    );\n  }\n};\n\nexport default Signup;\n","/Users/matthewgilligan/Code/matthewgilligan/gina-corrieri/src/components/forms/FormInput/index.js",[],"/Users/matthewgilligan/Code/matthewgilligan/gina-corrieri/src/pages/Recovery/index.js",[],"/Users/matthewgilligan/Code/matthewgilligan/gina-corrieri/src/components/AuthWrapper/index.js",[],"/Users/matthewgilligan/Code/matthewgilligan/gina-corrieri/src/components/EmailPassword/index.js",[],"/Users/matthewgilligan/Code/matthewg